pipeline {
    agent any

    stages {
        stage('Checkout GIT') {
            steps {
                echo 'Pulling...'
                git branch: 'master',
                    url: 'https://github.com/aminemezlini321/atlier22.git'
            }
        }

        stage('Testing maven') {
            steps {
                sh 'mvn -compile'
            }
        }
    }
}




