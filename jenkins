pipeline {
    agent any

    tools {
        jdk 'Java_home'
        maven 'MAVEN'
    }

    stages {

        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Build with Maven') {
            steps {
                sh 'mvn -v'
                sh 'mvn clean compile'
            }
        }
    }
}



